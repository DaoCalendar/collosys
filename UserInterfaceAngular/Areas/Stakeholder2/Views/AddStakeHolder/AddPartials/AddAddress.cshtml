<div class="form-horizontal " data-ng-controller="addressCntrl" data-ng-show="addData.Hierarchy.HasAddress">
    <div class="row-fluid">
        <div class="span5">
            <cs-input ui-if="addData.Hierarchy.HasAddress" options="$parent.stakeholderModel.Line1"
                ng-model="$parent.WizardData.FinalPostModel.Address.Line1"></cs-input>
        </div>
        <!--Line-2-->
        <div class="span5">
            <cs-input ui-if="addData.Hierarchy.HasAddress" options="$parent.stakeholderModel.Line2"
                ng-model="$parent.WizardData.FinalPostModel.Address.Line2"></cs-input>
        </div>
    </div>

    <div class="row-fluid">
        <div class="span5">
            <cs-input options="$parent.stakeholderModel.Line3"
                ng-model="$parent.WizardData.FinalPostModel.Address.Line3"></cs-input>
        </div>
        <!--Landline No-->
        <div class="control-group span5">
            <label class="control-label">Landline No</label>
            <div class="controls">
                <div class="input-prepend">
                    <span class="add-on"><i class="icon-phone"></i></span>
                    <span class="add-on">+91</span>
                    <input type="text" class="input-medium"
                        data-ng-pattern="/^[0-9]{11}$/"
                        name="landline"
                        data-ng-model="$parent.WizardData.FinalPostModel.Address.LandlineNo">
                </div>
                <div class="field-validation-error" data-ng-show="addressDetailsForm.landline.$error.pattern">{{val.Pattern(11)}}</div>
            </div>
        </div>
    </div>

    <!--Pincode-->
    <div class="row-fluid">
        <cs-input options="$parent.stakeholderModel.Pincode"
                  ng-model="addData.Pincode"
                  cs-typeahead="'c.Area for c in $parent.$parent.Pincodes($viewValue, 'Area')'"  
                  data-ng-change="setPincodeOnArea(addData.Pincode)" ></cs-input>
    </div>
</div>


