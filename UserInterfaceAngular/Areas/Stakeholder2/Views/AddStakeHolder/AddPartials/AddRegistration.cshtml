<div class="form-horizontal" data-ng-controller="Registration">
    <div class="row-fluid">
        <div class="span5">
            <cs-template options="$parent.stakeholderModel.TAN">
                <cs-text-field options="$parent.stakeholderModel.TAN" 
                               cs-model="$parent.WizardData.FinalPostModel.Registration.TanNo" >
                </cs-text-field>
            </cs-template>
        </div>
        
        <div class="span5">
            <cs-template options="$parent.stakeholderModel.PAN">
                <cs-text-field options="$parent.stakeholderModel.PAN" 
                               cs-model="$parent.WizardData.FinalPostModel.Registration.PanNo" >
                </cs-text-field>
            </cs-template>
        </div>
    </div>

    <div class="row-fluid">
               <div class="span5">
            <cs-template options="$parent.stakeholderModel.RegistrationNo">
                <cs-text-field options="$parent.stakeholderModel.RegistrationNo" 
                               cs-model="$parent.WizardData.FinalPostModel.Registration.RegistrationNo" >
                </cs-text-field>
            </cs-template>
        </div>
       
          <div class="span5">
            <cs-template options="$parent.stakeholderModel.ServiceTax">
                <cs-text-field options="$parent.stakeholderModel.ServiceTax" 
                               cs-model="$parent.WizardData.FinalPostModel.Registration.ServiceTaxno" >
                </cs-text-field>
            </cs-template>
        </div>
    </div>
</div>



