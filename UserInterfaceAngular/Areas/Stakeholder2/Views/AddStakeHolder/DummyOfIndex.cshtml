@{
    ViewBag.Title = "DummyOfIndex";
}
<div class="container-fluid">
    <div class="row-fluid" data-ng-cloak="" data-ng-controller="AddStakeHolderCtrl">

        <form name="formPayment" class="form-horizontal" novalidate>
            @*  Add Stakeholder  *@
            <div class="panel" data-ng-show="addStakeholderpanel1">
                <div class="panel-heading">
                    Add Stakeholder 
                </div>
                <div class="row-fluid" data-ng-form="addStakeholderFrom">
                    <div class="row-fluid">
                        <div class="control-group span6">
                            <label class="control-label">Hierarchy</label>
                            <div class="controls">
                                <select data-ng-model="SelectedHier.Vertical" class="input-large" data-ui-select2=""
                                    data-ng-requiered="true">
                                    <option value=""></option>
                                    <option data-ng-repeat="data in HierarchyList | unique : 'Hierarchy'" value="{{data.Hierarchy}}">{{data.Hierarchy}}</option>
                                </select>
                            </div>
                        </div>
                        <div class="control-group span6" data-ng-show="SelectedHier.Vertical">
                            <label class="control-label">Designation</label>
                            <div class="controls">
                                <select data-ng-model="SelectedHier.HierarchyId" class="input-large" data-ui-select2=""
                                    data-ng-change="changeHierarchy(SelectedHier)"
                                    data-ng-required="true">
                                    <option value=""></option>
                                    <option data-ng-repeat="data in HierarchyList | filter : {'Hierarchy' : SelectedHier.Hierarchy } | orderBy: 'PositionLevel'"
                                        value="{{data.Id}}">{{getHierarchyDisplayName(data)}}</option>
                                </select>
                            </div>
                        </div>
                    </div>

                    <div class="row-fluid text-right">
                        <button class="btn btn-info" data-ng-click="">Reset</button>
                        <button class="btn btn-success" data-ng-click="Next(2)" data-ng-disabled="!addStakeholderFrom.$valid">Next</button>
                        <button class="btn btn-warning" data-ng-click="Cancel()">Cancel</button>
                    </div>
                </div>
            </div>

            @* Basic basicInfo *@
            <div class="panel" data-ng-show="basicInfopanel2">
                <div class="panel-heading">
                    Basic basicInfo 
                </div>

                <div class="row-fluid" data-ng-form="basicInfoForm">
                    <div class="row-fluid">
                        <div class="row-fluid">
                            <div class="span5 control-group">
                                <label class="control-label">Name</label>
                                <div class="controls">
                                    <input type="text" class="input-large" data-ng-pattern="/^[a-zA-Z ]{1,100}$/" name="Name"
                                         data-ng-model="Stakeholder.Name" required />
                                    @*Name Validation*@
                                    <div class="error">
                                        <span class="field-validation-error" data-ng-show="basicInfoForm.Name.$error.pattern">{{val.ValidName('Name')}}</span>
                                    </div>
                                </div>
                            </div>
                            <div class="control-group span5">
                                <label class="control-label">User Id</label>
                                <div class="controls">
                                    <div class="input-prepend">
                                        <span class="add-on"><i class="icon-user"></i></span>
                                        <input type="number" min="0" data-ng-maxlength="7"
                                            data-ng-model="Stakeholder.ExternalId" name="UserIdBox" required>
                                    </div>
                                    <div class="error">
                                        <span class="field-validation-error" data-ng-show="basicInfoForm.UserIdBox.$error.maxlength">{{val.MaxLength('7')}}</span>
                                        <span class="field-validation-error" data-ng-show="basicInfoForm.UserIdBox.$error.min">{{ val.MinLength('0')}} </span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row-fluid">
                            <div class="span5 control-group">
                                <label class="control-label">Mobile No</label>
                                <div class="controls">
                                    <div class="input-prepend ">
                                        <span class=" add-on"><i class="icon-phone"></i></span>
                                        <span class="add-on">+91</span>
                                        <input type="text" class="input-medium" data-ng-minlength="10" name="MobileNo" data-ng-pattern="/^[0-9]{1,10}$/"
                                            data-ng-maxlength="10" data-ng-model="Stakeholder.MobileNo" required />
                                    </div>
                                    <div class="error">
                                        <span class="field-validation-error" data-ng-show="basicInfoForm.MobileNo.$invalid && basicInfoForm.MobileNo.$error.pattern">{{val.ValidName('Mobile Number')}} </span>
                                        <span class="field-validation-error" data-ng-show="basicInfoForm.MobileNo.$error.pattern">{{val.EqualLength('10')}}</span>
                                        <span class="field-validation-error" data-ng-show="basicInfoForm.MobileNo.$invalid && basicInfoForm.MobileNo.$error.minlength">{{ val.MinLength('10')}} </span>
                                    </div>
                                </div>
                            </div>

                            <div class="span5 control-group">
                                <label class="control-label">Email Id</label>
                                <div class="controls">
                                    <div class="input-prepend input-append">
                                        <div data-ng-show="isEmployee">
                                            <span class="add-on"><i class="icon-envelope"></i></span>
                                            <input type="email" class="input-large" data-ng-model="Stakeholder.EmailId"
                                                 name="email" data-ng-required="isEmployee" />
                                        </div>
                                        <div data-ng-show="!isEmployee">
                                            <span class="add-on"><i class="icon-envelope"></i></span>
                                            <input type="text" data-ng-pattern="/^[a-zA-Z0-9.]+$/" class="input-medium" name="scEmail" data-ng-model="EmailId" data-ng-required="!isEmployee" />
                                            <span class="add-on">@@sc.com</span>
                                        </div>
                                    </div>
                                    @*email validation*@
                                    <div class="field-validation-error" data-ng-show="(basicInfoForm.email.$invalid && basicInfoForm.email.$dirty)||(basicInfoForm.scEmail.$invalid && basicInfoForm.scEmail.$dirty)">
                                        <span data-ng-show="(basicInfoForm.email.$dirty && basicInfoForm.email.$invalid)||(basicInfoForm.scEmail.$error.pattern && basicInfoForm.scEmail.$invalid)">{{val.ValidName('Email')}}</span>
                                        <span class="field-validation-valid" data-ng-show="(basicInfoForm.email.$valid)||(basicInfoForm.scEmail.$valid)">{{val.ValidName('Email')}}</span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row-fluid">
                            <div class="span5 control-group">
                                <label class="control-label">Gender</label>
                                <div class="controls">
                                    <div class="input-prepend">
                                        <span class="add-on">
                                            <input type="radio" data-ng-model="Stakeholder.Gender" checked="checked" value="0"><label>Male</label>

                                            <input type="radio" data-ng-model="Stakeholder.Gender" value="1"><label>Female</label>
                                        </span>
                                    </div>
                                </div>
                            </div>
                            <div class="span5 control-group">
                                <label class="control-label">Date of Birth </label>
                                <div class="controls">
                                    <div class="input-append">
                                        <input readonly type="text" class="input-medium" data-ng-model="Stakeholder.BirthDate"
                                            data-date-format="dd-mm-yyyy" data-date-end-date="-18y" bs-datepicker>
                                        <button type="button" class="btn" data-toggle="datepicker"><i class="icon-calendar"></i></button>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="row-fluid">
                            <div class="span5 control-group">
                                <label class="control-label">Reporting Manager</label>
                                <div class="controls">
                                    <select class="input-medium" data-ui-select2="" data-ng-model="Stakeholder.ReportsTo">
                                        <option></option>
                                        <option data-ng-repeat="data in ReportsToList" value="{{data.Id}}">{{data.Name}}, {{data.Designation}}</option>
                                    </select>
                                </div>
                            </div>

                            <div class="span5 control-group">
                                <label class="control-label">Date Of Joining</label>
                                <div class="controls"> @*ui-validate="'$value > Stakeholder.BirthDate'"*@
                                    <div class="input-append">
                                        <input readonly type="text" class="input-medium" 
                                            data-date-start-date="-1y" data-date-end-date="+1y"
                                            data-ng-model="Stakeholder.JoiningDate" data-date-format="dd-mm-yyyy" bs-datepicker required>
                                        <button type="button" class="btn" data-toggle="datepicker"><i class="icon-calendar"></i></button>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                    {{basicInfoForm.$valid}}
                    <div class="row-fluid text-right">
                        <button class="btn btn-info" data-ng-click="">Reset</button>
                        <button class="btn btn-success" data-ng-click="Prev(2)">Prev</button>
                        <button class="btn btn-success" data-ng-click="Next(3)" data-ng-disabled="basicInfoForm.$invalid">Next</button>
                        <button class="btn btn-warning" data-ng-click="Cancel()">Cancel</button>
                    </div>
                </div>
            </div>

            @*   Registration *@
            <div class="panel" data-ng-show="registrationpanel3">
                <div class="panel-heading">
                    Registration
                </div>
                <div class="row-fluid" data-ng-form="registrationForm">
                    <div class="row-fluid">
                        @* TAN PAN *@
                        <div class="span5 control-group">
                            <div class="control-group">
                                <label class="control-label">TAN</label>
                                <div class="controls">
                                    <input name="tan" type="text" data-ng-model="Stakeholder.Registration.Tan" data-ng-maxlength="30" 
                                        data-ng-pattern="/^\-?\d+((\.|\,)\d+)?$/" />
                                    <div class="field-validation-error" data-ng-show="registrationForm.tan.$error.maxlength">{{val.MaxLength(30)}}</div>
                                    <div class="field-validation-error" data-ng-show="registrationForm.tan.$dirty && registrationForm.tan.$error.pattern">{{val.OnlyNumbers()}}</div>
                                </div>
                            </div>
                        </div>
                        <div class="span5 control-group">
                            <div class="control-group ">
                                <label class="control-label">PAN</label>
                                <div class="controls">
                                    <input type="text" data-ng-model="Stakeholder.Registration.Pan" name="pan" data-ng-pattern="/^([A-Z]{5})(\d{4})([a-zA-Z]{1})$/" required />
                                    <div class="field-validation-error" data-ng-show=" registrationForm.pan.$error.pattern">Follow Given Format</div>
                                    <div class="spancolor">Ex.AAAAA0000A</div>
                                </div>
                            </div>
                        </div>
                    </div>

                    @* Registration No Service Tax *@
                    <div class="row-fluid">
                        <div class="span5 control-group">
                            <div class="control-group ">
                                <label class="control-label">Registration No</label>
                                <div class="controls">
                                    <input type="text" data-ng-maxlength="30" data-ng-model="Stakeholder.Registration.Registration" name="registration" data-ng-pattern="/^[a-zA-Z0-9]+$/" />
                                    <div class="field-validation-error" data-ng-show="registrationForm.registration.$error.maxlength ">{{val.MaxLength(30)}}</div>
                                    <div class="field-validation-error" data-ng-show="registrationForm.registration.$error.pattern">special characters are not allowed</div>
                                </div>
                            </div>
                        </div>

                        <div class="span5 control-group">
                            <div class="control-group ">
                                <label class="control-label">Service Tax</label>
                                <div class="controls">
                                    <input type="text" name="service" data-ng-model="Stakeholder.Registration.service" data-ng-maxlength="30" 
                                        data-ng-pattern="/^\-?\d+((\.|\,)\d+)?$/" />
                                    <div class="field-validation-error" data-ng-show="registrationForm.service.$error.maxlength">{{val.MaxLength(30)}}</div>
                                    <div class="field-validation-error" data-ng-show=" registrationForm.service.$error.pattern">{{val.OnlyNumbers()}}</div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="row-fluid text-right">
                        <button class="btn btn-info" data-ng-click="">Reset</button>
                        <button class="btn btn-success" data-ng-click="Prev(3)">Prev</button>
                        <button class="btn btn-success" data-ng-click="Next(4)" data-ng-disabled="!registrationForm.$valid">Next</button>
                        <button class="btn btn-warning" data-ng-click="Cancel()">Cancel</button>
                    </div>
                </div>
            </div>

            @*  Payment Details *@
            <div class="panel" data-ng-show="paymentDetailspanel4">
                <div class="panel-heading">
                    Payment Details
                </div>
                <div class="row-fluid" data-ng-form="paymentDetailsForm">
                    <div class="span12">
                        <!--Date Range-->
                        <div class="row-fluid">
                            <h5 style="font-weight: normal;">Date Range</h5>
                            <hr />
                            <div class="row-fluid">
                                <div class="control-group span6">
                                    <label class="control-label">Start Date</label>
                                    <div class="controls">
                                        <input type="text" class="input-medium" name="StartDate" readonly
                                            data-ng-required="Hierarchy.HasPayment"
                                            data-ng-model="Stakeholder.Payment.StartDate"
                                            data-date-format="dd-mm-yyyy" bs-datepicker>
                                        <button type="button" class="btn" data-toggle="datepicker">
                                            <i class="icon-calendar"></i>
                                        </button>
                                    </div>
                                </div>
                                <div class="control-group span6">
                                    <label class="control-label">End Date</label>
                                    <div class="controls">
                                        <div class="">
                                            <input type="text" class="input-medium" readonly
                                                data-ng-required="Hierarchy.HasPayment"
                                                data-ng-model="Stakeholder.Payment.EndDate"
                                                data-date-format="dd-mm-yyyy"
                                                ui-validate="'OneMonthGreater($value, Stakeholder.Payment.StartDate)'" bs-datepicker>
                                            <button type="button" name="EndDate" class="btn" data-toggle="datepicker"><i class="icon-calendar"></i></button>
                                            <div class="error" data-ng-show="EndDate">
                                                <span class="error" data-ng-show="EndDate">{{DateOneMonthGreater}}{{ Stakeholder.Payment.StartDate|date:dd-mm-yyy}} </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!--Variable Payment-->
                        <div class="row-fluid" data-ng-show="!Hierarchy.HasVarible">
                            <h5 style="font-weight: normal;">Variable Pay</h5>
                            <hr />

                            <div class="row-fluid">
                                <div class="control-group span6">
                                    <label class="control-label">Collection Policy:</label>
                                    <div class="controls">
                                        <select data-ui-select2="" id="linerPolicy"
                                            data-ng-required="!Stakeholder.Payment.VariableWriteoff"
                                            data-ng-model="Stakeholder.Payment.VariableLiner"
                                            data-ng-options="c for c in LinerPolicyList" style="min-width: 200px;">
                                        </select>
                                    </div>
                                </div>
                                <div class="control-group span6">
                                    <label class="control-label">Recovery Policy:</label>
                                    <div class="controls">
                                        <select data-ui-select2="" id="writeoffPolicy" data-ng-required="!Stakeholder.Payment.VariableLiner"
                                            data-ng-model="Stakeholder.Payment.VariableWriteoff" data-ng-options="c for c in WriteoffPolicyList"
                                            style="min-width: 200px;">
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!--Fixed Pay-->
                        <div class="row-fluid" data-ng-show="!Hierarchy.HasFixed">
                            <h5 style="font-weight: normal;">Fixed Pay</h5>
                            <hr />
                            <div class="row-fluid">
                                <div class="row-fluid">
                                    <div class="control-group span6">
                                        <label class="control-label">Basic :</label>
                                        <div class="controls">
                                            <div class="input-prepend">
                                                <span class="add-on"><i class="icon-rupee"></i></span>
                                                <input type="number" name="FixpayBasic" data-ng-required="!Hierarchy.HasFixed" min="0"
                                                    data-ng-maxlength="5" data-ng-model="Stakeholder.Payment.FixpayBasic" />
                                            </div>
                                            <div class="error">
                                                <span class="field-validation-error" data-ng-show="formPayment.FixpayBasic.$error.maxlength">{{val.MaxLength('5')}}</span>
                                                <span class="field-validation-error" data-ng-show="formPayment.FixpayBasic.$error.min">{{ val.MinLength('0')}} </span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="control-group span6" data-ng-show="!Hierarchy.HasFixedIndividual">
                                        <label class="control-label">Hra :</label>
                                        <div class="controls">
                                            <div class="input-prepend">
                                                <span class="add-on"><i class="icon-rupee"></i></span>
                                                <input type="number" data-ng-required="!Hierarchy.HasFixedIndividual" min="0" data-ng-maxlength="5" data-ng-model="Stakeholder.Payment.FixpayHra" name="FixpayHra" />
                                            </div>
                                            <div class="error">
                                                <span class="field-validation-error" data-ng-show="formPayment.FixpayHra.$error.maxlength">{{val.MaxLength('5')}}</span>
                                                <span class="field-validation-error" data-ng-show="formPayment.FixpayHra.$error.min">{{ val.MinLength('0')}} </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row-fluid">
                                    <div class="control-group span6" data-ng-show="!Hierarchy.HasFixedIndividual">
                                        <label class="control-label">Other :</label>
                                        <div class="controls">
                                            <div class="input-prepend">
                                                <span class="add-on"><i class="icon-rupee"></i></span>
                                                <input type="number" data-ng-required="!Hierarchy.HasFixedIndividual" min="0" data-ng-maxlength="5" data-ng-model="Stakeholder.Payment.FixpayOther" name="FixpayOther" />
                                            </div>
                                            <div class="error">
                                                <span class="field-validation-error" data-ng-show="formPayment.FixpayOther.$error.maxlength">{{val.MaxLength('5')}}</span>
                                                <span class="field-validation-error" data-ng-show="formPayment.FixpayOther.$error.min">{{ val.MinLength('0')}} </span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="control-group span6 info" data-ng-show="false">
                                        <label class="control-label">Total:</label>
                                        <div class="controls">
                                            <div class="input-prepend">
                                                <span class="add-on"><i class="icon-rupee"></i></span>
                                                <input type="text" data-ng-model="Stakeholder.Payment.FixpayTotal" disabled value="{{Stakeholder.Payment.FixpayTotal=TotalPayment(Stakeholder.Payment.FixpayBasic,Stakeholder.Payment.FixpayHra,Stakeholder.Payment.FixpayOther)}}" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="row-fluid" data-ng-show="!Hierarchy.HasFixedIndividual">
                                <table class="table table-bordered table-hover" data-ng-show="Stakeholder.Payment.FixpayBasic">
                                    <thead>
                                        <tr>
                                            <th data-ng-show="Stakeholder.Payment.FixpayBasic">Basic</th>
                                            <th data-ng-show="Stakeholder.Payment.FixpayHra && Stakeholder.Payment.FixpayOther">Allowances</th>
                                            <th data-ng-show="Stakeholder.Payment.FixpayBasic">Employee PF</th>
                                            <th data-ng-show="Stakeholder.Payment.FixpayBasic">Employer PF</th>
                                            <th>Employee ESIC</th>
                                            <th>Employer ESIC</th>
                                            <th>Service Charge</th>
                                            <th>Service Tax</th>
                                            <th>Gross Pay</th>
                                        </tr>
                                        <tr>
                                            <th data-ng-show="Stakeholder.Payment.FixpayBasic"></th>
                                            <th data-ng-show="Stakeholder.Payment.FixpayHra && Stakeholder.Payment.FixpayOther"></th>
                                            <th data-ng-show="Stakeholder.Payment.FixpayBasic">Basic * {{FixedPay.EmployeePF}}%</th>
                                            <th data-ng-show="Stakeholder.Payment.FixpayBasic">Basic * {{FixedPay.EmployerPF}}%</th>
                                            <th>Basic * {{FixedPay.EmployeeESIC}}%</th>
                                            <th>Basic * {{FixedPay.EmployerESIC}}% </th>
                                            <th>Basic * {{FixedPay.ServiceCharge}}%</th>
                                            <th>Basic * {{FixedPay.ServiceTax}}% </th>
                                            <th></th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td data-ng-show="Stakeholder.Payment.FixpayBasic">{{Stakeholder.Payment.FixpayBasic}}</td>
                                            <td data-ng-show="Stakeholder.Payment.FixpayHra && Stakeholder.Payment.FixpayOther">{{Stakeholder.Payment.FixpayHra*1 + Stakeholder.Payment.FixpayOther*1}}
                                            </td>

                                            <td data-ng-show="Stakeholder.Payment.FixpayBasic">{{(SalDetails.pf*1)}}</td>
                                            <td data-ng-show="Stakeholder.Payment.FixpayBasic">{{(SalDetails.pf*1)}}</td>
                                            <td>{{SalDetails.esicEmployee}}</td>
                                            <td>{{SalDetails.esicEmployer}}</td>
                                            <td>{{SalDetails.serviceCharge}}</td>
                                            <td>{{SalDetails.serviceTax}}</td>
                                            <td>{{Stakeholder.Payment.FixpayTotal}}</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>

                        <!--Allowances-->
                        <div class="row-fluid" data-ng-show="!Hierarchy.HasMobileTravel">
                            <h5 style="font-weight: normal;">Reimbursements</h5>
                            <hr />

                            <div class="row-fluid">
                                <div class="control-group span6">
                                    <label class="control-label">Mobile:</label>
                                    <div class="controls">
                                        <input type="number" data-ng-required="!Hierarchy.HasMobileTravel"
                                            min="0" data-ng-maxlength="5" data-ng-model="Stakeholder.Payment.MobileElig" name="MobileElig" />
                                        <div class="error">
                                            <span class="field-validation-error" data-ng-show="paymentDetailsForm.MobileElig.$error.maxlength">{{val.MaxLength('5')}}</span>
                                            <span class="field-validation-error" data-ng-show="paymentDetailsForm.MobileElig.$error.min">{{ val.MinLength('0')}} </span>
                                        </div>
                                    </div>
                                </div>
                                <div class="control-group span6">
                                    <label class="control-label">Travel:</label>
                                    <div class="controls">
                                        <input type="number" data-ng-required="!Hierarchy.HasMobileTravel" min="0"
                                            data-ng-maxlength="5" data-ng-model="Stakeholder.Payment.TravelElig" name="TravelElig" />
                                        <div class="error">
                                            <span class="field-validation-error" data-ng-show="paymentDetailsForm.TravelElig.$error.maxlength">{{val.MaxLength('5')}}</span>
                                            <span class="field-validation-error" data-ng-show="paymentDetailsForm.TravelElig.$error.min">{{ val.MinLength('0')}} </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!--Bank details-->
                        <div class="row-fluid" data-ng-show="false">
                            <h5 style="font-weight: normal;">Bank Details</h5>
                            <hr />
                            <div class="row-fluid">
                                <div class="row-fluid">
                                    <div class="control-group span6">
                                        <label class="control-label">Bank Acc. No:</label>
                                        <div class="controls">
                                            <input type="text" data-ng-pattern="/^[0-9a-zA-Z]+$/" data-ng-maxlength="25"
                                                data-ng-minlength="11" data-ng-model="Stakeholder.Payment.BankAccNo" name="BankAccNo" />
                                            <div class="error">
                                                <span class="field-validation-error" data-ng-show="paymentDetailsForm.BankAccNo.$error.maxlength">{{Maxlength25}}</span>
                                                <span class="field-validation-error" data-ng-show="paymentDetailsForm.BankAccNo.$error.minlength">{{Minlength11}}</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="control-group span6" data-ng-show="Stakeholder.Payment.BankAccNo">
                                        <label class="control-label">Bank Name:</label>
                                        <div class="controls">
                                            <input type="text" data-ng-required="Stakeholder.Payment.BankAccNo"
                                                data-ng-maxlength="100" data-ng-model="Stakeholder.Payment.BankAccName" name="BankAccName" />
                                            <div class="error">
                                                <span class="field-validation-error" data-ng-show="paymentDetailsForm.BankAccName.$error.maxlength">{{Maxlength100}}</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row-fluid" data-ng-show="Stakeholder.Payment.BankAccNo && Stakeholder.Payment.BankAccName">
                                    <div class="control-group span6">
                                        <label class="control-label">Bank IFSC No:</label>
                                        <div class="controls">
                                            <input type="text" data-ng-required="Stakeholder.Payment.BankAccNo && Stakeholder.Payment.BankAccName" d
                                                ata-ng-pattern="/^[A-Z][A-Z][A-Z][A-Z][0-9][0-9][0-9][0-9][0-9][0-9][0-9]$/" value=" "
                                                data-ng-model="Stakeholder.Payment.BankIfscCode" name="BankIfscCode" placeholder="Ex : AAAA0000000" />
                                            <div class="error">
                                                <span class="field-validation-error" data-ng-show="paymentDetailsForm.BankIfscCode.$error.pattern">Enter valid IFSC no</span>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <!--Other Pay-->
                        <div class="row-fluid" data-ng-show="!Hierarchy.HasServiceCharge">
                            <h5 style="font-weight: normal;">Other Charges</h5>
                            <hr />
                            <div class="row-fluid">
                                <div class="control-group span6">
                                    <label class="control-label">Service Charge:</label>
                                    <div class="controls">
                                        <div class="input-append">
                                            <input type="text"
                                                data-ng-required="!Hierarchy.HasServiceCharge"
                                                data-ng-pattern="/^100$|^\d{0,2}(\.\d{1,2})? *%?$/"
                                                data-ng-maxlength="99.99"
                                                class="input-mini"
                                                data-ng-model="Stakeholder.Payment.ServiceCharge" name="ServiceCharge" />
                                            <span class="add-on">%</span>
                                        </div>
                                        <div class="error">
                                            <span class="field-validation-error" data-ng-show="formPayment.ServiceCharge.$error.pattern">Enter valid percentage</span>
                                            <span class="field-validation-error" data-ng-show="formPayment.ServiceCharge.$error.maxlength">Max Length 99.99</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="control-group span6">
                            <button class="btn-success" data-ng-click="AddPayment(Stakeholder.Payment)">Add Payment</button>
                        </div>
                    </div>

                    <div class="row-fluid">

                        <table class="table">
                            <tbody>
                                <tr data-ng-repeat="data in PaymentList">
                                    <td>{{data.StartDate}}</td>
                                    <td>{{data.EndDate}}</td>
                                    <td>{{data.VariableLiner}}</td>
                                    <td>{{data.VariableWriteoff}}</td>
                                    <td>{{data.FixpayTotal}}</td>
                                    <td>{{data.MobileElig}}</td>
                                    <td>{{data.TravelElig}}</td>
                                    <td>{{data.MobileElig}}</td>
                                    <td>{{data.BankAccNo}}</td>
                                    <td>{{data.BankAccName}}</td>
                                    <td>{{data.BankIfscCode}}</td>
                                    <td>{{data.BankAccName}}</td>
                                    <td>{{data.ServiceCharge}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>

                    <div class="row-fluid text-right">
                        <button class="btn btn-info" data-ng-click="clear(basicInfo)">Reset</button>
                        <button class="btn btn-success" data-ng-click="Prev(4)">Prev</button>
                        <button class="btn btn-success" data-ng-click="Next(5)" data-ng-disabled="!paymentDetailsForm.$valid">Next</button>
                        <button class="btn btn-warning" data-ng-click="Cancel()">Cancel</button>
                    </div>
                </div>
            </div>

            @*   Payment Working  *@
            <div class="panel" data-ng-show="paymentWorkingpanel5">
                <div class="panel-heading">
                    Payment Working 
                </div>
                <div class="row-fluid" data-ng-form="paymentWorkinForm">
                    <div class="row-fluid">
                        <div class="control-group span5">
                            <label class="control-label">Line 1</label>
                            <div class="controls">
                                <input type="text" ng-maxlength="100" ng-model="Stakeholder.Address.Line1" required />
                            </div>
                        </div>
                        <div class="control-group span5">
                            <label class="control-label">Line 2</label>
                            <div class="controls">
                                <input type="text" class="input-large" ng-model="Stakeholder.Address.Line2" ng-maxlength="100" required>
                            </div>
                        </div>
                    </div>

                    <div class="row-fluid">
                        <div class="control-group span5">
                            <label class="control-label">Line 3</label>
                            <div class="controls">
                                <input type="text" ng-maxlength="100" ng-model="Stakeholder.Address.Line3" />
                            </div>
                        </div>
                        <div class="control-group span5">
                            <label class="control-label">Landline No</label>
                            <div class="controls">
                                <div class="input-prepend">
                                    <span class="add-on"><i class="icon-phone"></i></span>
                                    <span class="add-on">+91</span>
                                    <input type="text" class="input-medium" ng-pattern="/^[0-9]{11}$/" name="landline" 
                                        ng-model="Stakeholder.Address.LandlineNo">
                                </div>
                                <div ng-show="paymentWorkinForm.landline.$invalid && paymentWorkinForm.landline.$dirty || paymentWorkinForm.pincode.$valid">
                                    <span class="field-validation-error" ng-show="paymentWorkinForm.landline.$invalid && paymentWorkinForm.landline.$error.pattern"><i class="icon-remove"></i>{{ val.ValidName('LandLine Number')}}</span>
                                    <span class="field-validation-valid" ng-show="paymentWorkinForm.landline.$valid"><i class="icon-ok"></i>Valid </span>
                                </div>

                            </div>
                        </div>
                    </div>

                    <div class="row-fluid">
                        <div class="control-group span5">
                            <label class="control-label">Pincode</label>
                            <div class="controls">
                                <input type="text"
                                    class="input-medium"
                                    data-ng-model="Stakeholder.Address.PincodeNew"
                                    typeahead="c.Area for c in Pincodes($viewValue, 'Area')" required>
                            </div>

                            <div ng-show="paymentWorkinForm.pincode.$invalid && paymentWorkinForm.pincode.$dirty ">
                                <span class="field-validation-error" ng-show="paymentWorkinForm.pincode.$invalid && paymentWorkinForm.pincode.$error.pattern"><i class="icon-remove"></i>Invalid Pincode</span>
                            </div>
                        </div>
                    </div>

                    <div class="row-fluid text-right">
                        <button class="btn btn-info" data-ng-click="clear(basicInfo)">Reset</button>
                        <button class="btn btn-success" data-ng-click="Prev(5)">Prev</button>
                        <button class="btn btn-success" data-ng-click="SaveData(Stakeholder,SelectedHier)" data-ng-disabled="!paymentWorkinForm.$valid">Save</button>
                        <button class="btn btn-warning" data-ng-click="Cancel()">Cancel</button>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>