<!-- Row 1 for name and external id-->
<div class="row-fluid">
    <div class="control-group span6">
        <label class="control-label">Name:</label>
        <div class="controls">
            <input type="text" ng-model="Stakeholder.Name" readonly placeholder="Enter Name" />
        </div>
    </div>
    <div class="control-group span6" ng-show="Hierarchy.IsUser">
        <label class="control-label">User ID</label>
        <div class="controls">
            <input type="text" readonly ng-model="Stakeholder.ExternalId" />
        </div>
    </div>
</div>

<!-- Row 2 mobile no and email id-->
<div class="row-fluid">
    <div class="control-group span6">
        <label class="control-label">Mobile no:</label>
        <div class="controls">
            <input type="text" readonly ng-model="Stakeholder.MobileNo" />
        </div>
    </div>
    <div class="control-group span6">
        <label class="control-label">Email:</label>
        <div class="controls">
            <input type="email" readonly ng-model="Stakeholder.EmailId" />
        </div>
    </div>
</div>

<!-- Row 3 for date of joining and date of leaving-->
<div class="row-fluid">
    <div class="control-group span6">
        <label class="control-label">Date of Joining:</label>
        <div class="controls">
            @*<input type="text" readonly ng-model="Stakeholder.JoiningDate |date:'yyyy-MM-dd HH:mm:ss Z'" data-date-format="dd-mm-yyyy" />*@
            <input type="text" readonly="readonly" value="{{Stakeholder.JoiningDate |date:'dd-MM-yyyy'}}" />
        </div>
    </div>
    <div class="control-group span6" data-ng-form="dateleaving">
        <label class="control-label">Date of Leaving:</label>
        <div class="controls">
            <div class="input-append">
                <!--ui-validate="'$value > Stakeholder.JoiningDate'"-->
                <!-- ui-validate=" '$value | date:'yyyyMMdd' > Stakeholder.JoiningDate |date:'yyyyMMdd' ' "-->
                <!--moment($value).ticks > moment(Stakeholder.JoiningDate)-->
                <input type="text"
                    id="leavingId" name="leavingId"
                    class="input-medium"
                    ui-validate="'checkDates($value, Stakeholder.JoiningDate,dateleaving)'"
                    data-ng-model="Stakeholder.LeavingDate"
                    data-date-format="dd-MM-yyyy" bs-datepicker>
                <button type="button" class="btn" data-toggle="datepicker"><i class="icon-calendar"></i></button>
            </div>
        </div>
    </div>

</div>

<div class="row-fluid" data-ng-show="Hierarchy.ManageReportsTo">
    <div class="control-group span6">
        <label class="control-label">Reports To:</label>
        <div class="controls">
            <select id="Reportsto" class="input-medium" 
                    data-ng-model="Stakeholder.ReportsTo" 
                    ui-select2 
                    data-ng-required="Hierarchy.ManageReportsTo">
                <option></option>
                <option data-ng-repeat="data in ReportsToList" 
                        value="{{data.Id}}"> {{data.Name}},{{data.Designation}}</option>
            </select>
            
        </div>
       
    </div>
</div>

