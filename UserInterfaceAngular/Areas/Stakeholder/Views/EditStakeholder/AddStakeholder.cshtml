 <div class="row-fluid ng-cloak" ng-cloak ng-controller="hierarchy">
        <div class="row-fluid">
            
            <div class="text-right text-error">
                * Mandatory fields
            </div>
        </div>
        <form name="form" class="form-horizontal" novalidate>
            <div class="row-fluid" ng-form="formhierarchy">
                <div class="row-fluid">
                    <div class="panel">
                        <div class="panel-heading">Designation</div>
                        <!--Designation and Hierarchy-->
                        <div class="row-fluid">
                            <div class="control-group span6">
                                <label class="control-label">Designation</label>
                                <div class="controls">
                                    <select ng-model="Hierarchy" ng-options="c.Designation for c in stakeHierarchy " required data-ng-change="getReportsTo(Hierarchy)">
                                        <option value="">-- Select Designation --</option>
                                    </select>
                                </div>
                            </div>
                            <div class="control-group span6">
                                <label class="control-label">Vertical</label>
                                <div class="controls">
                                    <input type="text" ng-model="Hierarchy.Hierarchy" readonly/>    
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!--Stakeholder Controller -->
            <div class="row-fluid" ng-controller="stakeholder">
            
                <!-- Stakeholder details -->
                <div class="panel" ng-show="$parent.Hierarchy">
                    <div class="panel-heading">
                        Details
                    </div>
                     <div class="row-fluid" ng-form="formDetails">
                       
                            <!-- Row 1 for name and external id-->
                            <div class="row-fluid">
                                <div class="control-group span6">
                                    <label class="control-label">Name:</label>
                                    <div class="controls">
                                        <input type="text" required ng-model="Stakeholder.Name"  placeholder="Enter Name"/>
                                    </div>
                                </div>
                                <div class="control-group span6" ng-show="$parent.Hierarchy.IsUser">
                                    <label class="control-label">User ID</label>
                                    <div class="controls">
                                        <div class="input-prepend">
                                            <span class="add-on"><i class="icon-user"></i></span>
                                            <input type="text" required ng-pattern="/^[0-9][0-9][0-9][0-9][0-9][0-9][0-9]$/" ng-model="Stakeholder.ExternalId" required placeholder="Enter Employee id"/>
                                        </div>
                                        <label class="help-inline alert-error" ng-show="!checkUserID(Stakeholder.ExternalId)">User ID is exist in database</label>
                                    </div>
                                </div>
                            </div>
            
                            <!-- Row 2 mobile no and email id-->
                            <div class="row-fluid">
                                <div class="control-group span6">
                                    <label class="control-label">Mobile no:</label>
                                    <div class="controls">
                                        <input type="text" required ng-pattern="/^[0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]$/" ng-model="Stakeholder.MobileNo"/>
                                    </div>
                                </div>
                                <div class="control-group span6">
                                    <label class="control-label">Email:</label>
                                    <div class="controls">
                                        <div class="input-prepend">
                                            <span class="add-on"><i class="icon-envelope"></i></span>
                                            <input type="email" required ng-model="Stakeholder.EmailId"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
            
                            <!--row 3 for gender and date of birth-->
                            <div class="row-fluid" ng-show="$parent.Hierarchy.IsIndividual">
                                <div class="control-group span6">
                                    <label class="control-label">Gender</label>
                                    <div class="controls">
                                        <input type="radio" name="gender" ng-model="Stakeholder.Gender" value="Male"><label> Male</label>
                                        <input type="radio" name="gender"  ng-model="Stakeholder.Gender" value="Female" ng-checked="Stakeholder.Gender"><label> Female</label>
                                    </div>
                                </div>
                                <div class="control-group span6">
                                    <label class="control-label">DOB</label>
                                    <div class="controls">
                                        <div class="input-append">
                                            <input type="text" class="input-medium" ng-required="$parent.Hierarchy.IsIndividual" data-ng-model="Stakeholder.DateOfBirth" data-date-format="dd-mm-yyyy" bs-datepicker>
                                            <button type="button" class="btn" data-toggle="datepicker"><i class="icon-calendar"></i></button>               
                                        </div>
                                    </div>
                                   @* <div class="controls">
                                        <input type="date" ng-required="$parent.Hierarchy.IsIndividual" ng-model="Stakeholder.DateOfBirth"/>
                                    </div>*@
                                </div>
                            </div>

                            <!-- Row 4 for date of joining -->
                            <div class="row-fluid">
                                <div class="control-group span6">
                                    <label class="control-label">Date of Joining:</label>
                                    <div class="controls">
                                        <div class="input-append">
                                            <input type="text" class="input-medium" required data-ng-model="Stakeholder.DateOfJoining" data-date-format="dd-mm-yyyy" bs-datepicker>
                                            <button type="button" class="btn" data-toggle="datepicker"><i class="icon-calendar"></i></button>               
                                        </div>
                                    </div>
                                   @* <div class="controls">
                                        <input type="date" required ng-model="Stakeholder.DateOfJoining"/>
                                    </div>*@
                                </div>
                                <div class="control-group span6" ng-show="$parent.Hierarchy.ManageReportsTo">
                                    <label class="control-label">Reports To:</label>
                                    <div class="controls">
                                        <select id="Reportsto"  ng-model="Stakeholder.ReportsTo">
                                            <option ng-repeat="data in $parent.ReportsToList" value="{{data.Id}}">{{data.Name}}</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                       
                    </div>
                </div>
                   
                 <!-- Stakeholder Registration-->
                <div class="panel" ng-show="$parent.Hierarchy.HasRegistration">
                    
                    <div class="panel-heading">
                        Registration
                    </div>
                    <div class="row-fluid" ng-form="formReg" id="stake-registraion" >
                        <!-- Row 1 tan no and pan no -->
                        <div class="row-fluid">
                            <div class="control-group span6">
                                <label class="control-label">TAN:</label>
                                <div class="controls">
                                    <input type="text" ng-required="$parent.Hierarchy.HasRegistration" ng-pattern="/^[A-Z][A-Z][A-Z][A-Z][0-9][0-9][0-9][0-9][0-9][A-Z]$/" ng-model="Stakeholder.Registration.TanNo"/>
                                    <label class="help-inline alert-error" ng-show="!checkTanNo(Stakeholder.Registration.TanNo)">Tan no is exist in database</label>
                                    <label class="help-inline">Ex: AAAA00000A</label>
                                </div>
                            </div>
                            <div class="control-group span6">
                                <label class="control-label">PAN:</label>
                                <div class="controls">
                                    <input type="text"  ng-required="$parent.Hierarchy.HasRegistration" ng-pattern="/^[A-Z][A-Z][A-Z][A-Z][A-Z][0-9][0-9][0-9][0-9][A-Z]$/" ng-model="Stakeholder.Registration.PanNo"/>
                                    <label class="help-inline alert-error" ng-show="!checkPanNo(Stakeholder.Registration.PanNo)">Pan no is exist in database</label>
                                    <label class="help-inline">Ex: AAAAA0000A</label>
                                </div>
                            </div>
                        </div>
                    
                        <!-- Row 2  registration no and service tax no-->
                        <div class="row-fluid">
                            <div class="control-group span6">
                                <label class="control-label">Registration:</label>
                                <div class="controls">
                                    <input type="text" ng-required="$parent.Hierarchy.HasRegistration" ng-pattern="/^[A-Z][A-Z][0-9][0-9][0-9][0-9][0-9][0-9][0-9][0-9]$/" ng-model="Stakeholder.Registration.RegistrationNo"/>
                                    <label class="help-inline alert-error" ng-show="!checkRegNo(Stakeholder.Registration.RegistrationNo)">Registration no is exist in database</label>
                                    <label>Ex: AA00000000</label>
                                </div>
                            </div>
                            <div class="control-group span6">
                                <label class="control-label">Service Tax:</label>
                                <div class="controls">
                                    <input type="text" ng-required="$parent.Hierarchy.HasRegistration" ng-pattern="/^[A-Z][A-Z][A-Z][A-Z][A-Z][0-9][0-9][0-9][0-9][A-Z][S][T][0][0][1-9]$/" ng-model="Stakeholder.Registration.ServiceTaxno"/>
                                    <label class="help-inline alert-error" ng-show="!checkServiceNo(Stakeholder.Registration.ServiceTaxno)">ServiceTax no is exist in database</label>
                                    <label>Ex: AAAAA0000AST001</label>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                 <!-- Stakeholder Working -->
                <div class="panel" ng-show="$parent.Hierarchy.HasWorking">
                    <div class="panel-heading">
                        Working
                    </div>
                    <div class="row-fluid" ng-form="formWorking" id="stake-working" >
                    
                        <!--Row for display lables on top of dropdowns-->
                        <div class="row-fluid">
                            <div class="span2">
                                <label>Product</label>
                            </div>

                            <div class="span2" ng-show="$parent.Hierarchy.LocationLevel=='Region'">
                                <label>Region</label>
                            </div>
                        
                            <div class="span2" ng-show="$parent.Hierarchy.LocationLevel=='Cluster'">
                                <label>State</label>
                            </div>

                            <div class="span2" ng-show="$parent.Hierarchy.LocationLevel=='Cluster'">
                                <label>Cluster</label>
                            </div>

                            <div class="span2" ng-show="$parent.Hierarchy.LocationLevel=='City'">
                                <label>City</label>
                            </div>

                            <div class="span2" ng-show="$parent.Hierarchy.HasBuckets">
                                <label>Bucket</label>
                            </div>
                        </div>

                        <!---row contains dropdowns-->
                        <div class="row-fluid" >
                            <div class="span2">
                                <select id="product" class="select-working" required  ng-model="Stakeholder.Working.Products" ng-options="c for c in ProductList">
                                </select>
                            </div>
                            
                            <div class="span2" ng-show="$parent.Hierarchy.LocationLevel=='Region'">
                                <select id="Region" class="select-working" ng-required="$parent.Hierarchy.LocationLevel=='Region'" ng-model="Stakeholder.Working.Region">
                                    <option ng-repeat="data in LocationList | unique:'Region'" value="{{data.Region}}">{{data.Region}}</option>
                                </select>
                            </div>

                            <div class="span2" ng-show="$parent.Hierarchy.LocationLevel=='Cluster'">
                                <select id="state" class="select-working" ng-required="$parent.Hierarchy.LocationLevel=='Cluster'" ng-model="Stakeholder.Working.State"  ng-options="c.State for c in LocationList | unique:'State'"></select>
                            </div>
                            
                            <div class="span2" ng-show="$parent.Hierarchy.LocationLevel=='Cluster'">
                                <select id="Cluster" class="select-working" ng-required="$parent.Hierarchy.LocationLevel=='Cluster'" ng-model="Stakeholder.Working.Cluster" ng-options="c.Cluster for c in LocationList | unique:'Cluster'">
                                </select>
                            </div>
                            
                            <div class="span2" ng-show="$parent.Hierarchy.LocationLevel=='City'">
                                <select id="City" class="select-working" ng-required="$parent.Hierarchy.LocationLevel=='City'" ng-model="Stakeholder.Working.City" ng-options="c.City for c in LocationList | unique:'City'">
                                </select>
                            </div>
                            
                            <div class="span2" ng-show="$parent.Hierarchy.HasBuckets">
                                <select id="Bucket" class="select-working" ng-required="$parent.Hierarchy.HasBuckets" ng-model="Stakeholder.Working.BucketStart" ng-options="c for c in BucketList">
                                </select>
                            </div>

                            <div class="span2">
                                <input type="button" ng-disabled="!formWorking.$valid" value="Add Working" class="btn btn-success" ng-click="AddWorking(Stakeholder,$parent.Hierarchy)" />
                            </div>
                        </div>
                    
                        <!--row for table to show working list-->
                        <div class="row-fluid" ng-show="WorkingList.length>0">
                            <table class="table table-bordered table-hover">
                                <thead>
                                    <tr class="success">
                                        <th>Delete</th>
                                        <th>Product</th>
                                        <th>Region</th>
                                        <th>Cluster</th>
                                        <th>State</th>
                                        <th>District</th>
                                        <th>City</th>
                                        <th>Bucket</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="data in WorkingList" class="info">
                                        <td>
                                            <div class="btn-group">
                                                <i class="btn icon-trash" data-ng-click="DeleteWorking($index)"></i>
                                            </div>
                                        </td>
                                        <td>
                                            {{data.Products}}
                                        </td>    
                                        <td>
                                            {{data.Region}}
                                        </td>
                                        <td>
                                            {{data.Cluster}}
                                        </td>
                                        <td>
                                            {{data.State}}
                                        </td>
                                        <td>
                                            {{data.District}}
                                        </td>
                                        <td>
                                            {{data.City}}
                                        </td>
                                        <td>
                                            {{data.BucketStart}}
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>

                    </div>
                </div>

                <div class="panel" ng-show="$parent.Hierarchy.HasPayment">
                    <div class="panel-heading">
                        Payment Details
                    </div>
                    <!--Stakeholder Payment-->
                    <div class="row-fluid" ng-form="formPayment" id="stake-payment" >
                        <div class="span12" >
                       
                            <!--Variable Payment-->
                            <div class="panel panel-info" ng-show="$parent.Hierarchy.HasVarible">
                                <div class="panel-heading">
                                    Variable Pay
                                </div>
                                
                                <div class="row-fluid" >
                                    <div class="control-group span6">
                                        <label class="control-label">Liner Policy:</label>
                                  
                                        <div class="controls">
                                            <select id="linerPolicy" ng-required="$parent.Hierarchy.HasVarible" ng-model="Stakeholder.Payment.VariableLiner" ng-options="c for c in LinerPolicyList" style="min-width: 200px;"></select>
                                        </div>
                                    </div>
                                    <div class="control-group span6">
                                        <label class="control-label">WriteOff Policy:</label>
                                   
                                        <div class="controls">
                                            <select id="writeoffPolicy" ng-required="$parent.Hierarchy.HasVarible" ng-model="Stakeholder.Payment.VariableWriteoff" ng-options="c for c in WriteoffPolicyList" style="min-width: 200px;"></select>
                                        </div>
                                    </div>

                                </div>
                            </div>
                            
                            <!--Fixed Pay-->
                            <div class="panel panel-info" ng-show="$parent.Hierarchy.HasFixed">
                                <div class="panel-heading">
                                    Fixed Pay
                                </div>
                                
                                <div class="row-fluid" >
                                    <div class="row-fluid">
                                        <div class="control-group span6">
                                            <label class="control-label">Fixpay Basic:</label>
                                      
                                            <div class="controls">
                                                <input type="text" ng-required="$parent.Hierarchy.HasFixed" ng-pattern="/^[0-9]+$/" ng-maxlength="8" ng-model="Stakeholder.Payment.FixpayBasic"/>
                                            </div>
                                        </div>
                                        <div class="control-group span6"  ng-show="$parent.Hierarchy.HasFixedIndividual">
                                            <label class="control-label">Fixpay Hra:</label>
                                       
                                            <div class="controls">
                                                <input type="text" ng-required="$parent.Hierarchy.HasFixedIndividual" ng-pattern="/^[0-9]+$/" ng-maxlength="8" ng-model="Stakeholder.Payment.FixpayHra"/>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row-fluid">
                                        <div class="control-group span6"  ng-show="$parent.Hierarchy.HasFixedIndividual">
                                            <label class="control-label">Fixpay Other:</label>
                                       
                                            <div class="controls">
                                                <input type="text" ng-required="$parent.Hierarchy.HasFixedIndividual" ng-pattern="/^[0-9]+$/" ng-maxlength="8" ng-model="Stakeholder.Payment.FixpayOther"/>
                                            </div>
                                        </div>
                                        <div class="control-group span6 info" ng-show="$parent.Hierarchy.HasFixedIndividual">
                                            <label class="control-label">Total:</label>    
                                            <div class="controls">
                                                <input type="text" ng-model="Stakeholder.Payment.FixpayTotal"  readonly value="{{Stakeholder.Payment.FixpayTotal=TotalPayment(Stakeholder.Payment.FixpayBasic,Stakeholder.Payment.FixpayHra,Stakeholder.Payment.FixpayOther)}}" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            
                             <!--Other Pay-->
                            <div class="panel panel-info" ng-show="$parent.Hierarchy.HasServiceCharge">
                                <div class="panel-heading">
                                    Other Charges
                                </div>
                               
                                <div class="row-fluid" >
                                    <div class="control-group span6" >
                                        <label class="control-label">Service Charge:</label>
                                        <div class="controls">
                                            <input type="text" ng-required="$parent.Hierarchy.HasServiceCharge" ng-pattern="/^[0-9]+$/" ng-maxlength="8" ng-model="Stakeholder.Payment.ServiceCharge"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            
                             <!--Allowances-->
                            <div class="panel panel-info" ng-show="$parent.Hierarchy.HasMobileTravel">
                                <div class="panel-heading">
                                    Allowances
                                </div>
                               
                                <div class="row-fluid" >
                                    <div class="control-group span6">
                                        <label class="control-label">Mobile:</label>
                                        <div class="controls">
                                            <input ng-required="$parent.Hierarchy.HasMobileTravel" ng-pattern="/^[0-9]+$/" ng-maxlength="8" type="text" ng-model="Stakeholder.Payment.MobileElig"/>
                                        </div>
                                    </div>
                                    <div class="control-group span6">
                                        <label class="control-label">Travel:</label>
                                        <div class="controls">
                                            <input type="text" ng-required="$parent.Hierarchy.HasMobileTravel" ng-pattern="/^[0-9]+$/" ng-maxlength="8" ng-model="Stakeholder.Payment.TravelElig"/>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            
                             <!--Date Range-->
                            <div class="panel panel-info">
                                <div class="panel-heading">
                                    Date Range
                                </div>
                               
                                <div class="row-fluid">
                                    <div class="control-group span6">
                                        <label class="control-label">Start Date</label>
                                        <div class="controls">
                                            <div class="input-append">
                                                <input type="text" class="input-medium" ng-required="$parent.Hierarchy.HasPayment" ui-validate=" '$value >= Stakeholder.DateOfJoining' " data-ng-model="Stakeholder.Payment.StartDate" data-date-format="dd-mm-yyyy" bs-datepicker>
                                                <button type="button" class="btn" data-toggle="datepicker"><i class="icon-calendar"></i></button>               
                                            </div>
                                            @* <label class="help-inline alert-error" ng-show=" !(Stakeholder.Payment.StartDate >= Stakeholder.DateOfJoining )">Start date should be greater than or equal to Date of Joining</label>*@
                                        </div>
                                        @* <div class="controls">
                                        <input type="date" id="startDate" ng-required="$parent.Hierarchy.HasPayment" ng-model="Stakeholder.Payment.StartDate"/>
                                    </div>*@
                                    </div>
                                    <div class="control-group span6">
                                        <label class="control-label">End Date</label>
                                        <div class="controls">
                                            <div class="input-append">
                                                <input type="text" class="input-medium" ng-required="$parent.Hierarchy.HasPayment" ui-validate=" '$value > Stakeholder.Payment.StartDate' " ng-model="Stakeholder.Payment.EndDate" data-date-format="dd-mm-yyyy" bs-datepicker>
                                                <button type="button" class="btn" data-toggle="datepicker"><i class="icon-calendar"></i></button>               
                                            </div>
                                        </div>
                                        @*  <div class="controls">
                                        <input type="date" id="endDate" ng-required="$parent.Hierarchy.HasPayment" ui-validate=" '$value > Stakeholder.Payment.StartDate' " required ng-model="Stakeholder.Payment.EndDate"/>
                                    </div>*@
                                    </div>
                                </div>
                            </div>
                            
                            <!--Bank details-->
                            <div class="panel panel-info" ng-show="$parent.Hierarchy.HasBankDetails">
                                <div class="panel-heading">
                                    Bank Details
                                </div>
                                
                                <div class="row-fluid" >
                                    <div class="row-fluid">
                                        <div class="control-group span6">
                                            <label class="control-label">Bank Acc. No:</label>
                                            <div class="controls">
                                                <input type="text" ng-required="$parent.Hierarchy.HasBankDetails" ng-pattern="/^[0-9]+$/" ng-maxlength="11" ng-model="Stakeholder.Payment.BankAccNo"/>
                                            </div>
                                        </div>
                                        <div class="control-group span6">
                                            <label class="control-label">Bank Name:</label>
                                            <div class="controls">
                                                <input type="text" ng-required="$parent.Hierarchy.HasBankDetails" ng-model="Stakeholder.Payment.BankAccName"/>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row-fluid">
                                        <div class="control-group span6">
                                            <label class="control-label">Bank IFSC No:</label>
                                            <div class="controls">
                                                <input type="text" ng-required="$parent.Hierarchy.HasBankDetails" ng-pattern="/^[A-Z][A-Z][A-Z][A-Z][0-9][0-9][0-9][0-9][0-9][0-9][0-9]$/" ng-model="Stakeholder.Payment.BankIfscCode"/>
                                            </div>
                                        </div>
                                        <div class="control-group span6">
                                            <label class="control-label">Ex : AAAA0000000</label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                           
                            <!--Add payment-->
                            <div class="row-fluid text-center">
                                @* <div class="control-group">
                                    <div class="controls">*@
                                <input type="button" ng-disabled="!formPayment.$valid" class="btn btn-success" value="Add Payment" ng-click="AddPayment(Stakeholder)"/>
                                @* </div>
                                </div>*@
                            </div>
                            
                            <!--table for displaying added payments-->
                            <div class="row-fluid" ng-show="PaymentList.length>0">
                                <table class="table table-bordered table-hover">
                                    <thead>
                                        <tr class="success">
                                            <th>Delete</th>
                                            <th ng-show="$parent.Hierarchy.HasMobileTravel">Mobile Elg.</th>
                                            <th ng-show="$parent.Hierarchy.HasMobileTravel">Travel Elg.</th>
                                            <th ng-show="$parent.Hierarchy.HasVarible">Liner Policy</th>
                                            <th ng-show="$parent.Hierarchy.HasVarible">WriteOff Policy</th>
                                            <th ng-show="$parent.Hierarchy.HasFixedSummary">Fixed Basic</th>
                                            <th ng-show="$parent.Hierarchy.HasFixedIndividual">Fixed Basic</th>
                                            <th ng-show="$parent.Hierarchy.HasFixedIndividual">Fixed Hra</th>
                                            <th ng-show="$parent.Hierarchy.HasFixedIndividual">Fixed Other</th>
                                            <th ng-show="$parent.Hierarchy.HasServiceCharge">Service Charge</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr class="info" ng-repeat="c in PaymentList">
                                            <td>
                                                <div class="btn-group">
                                                    <i class="btn icon-trash" data-ng-click="DeletePayment($index)"></i>
                                                </div>
                                            </td>
                                            <td ng-show="$parent.Hierarchy.HasMobileTravel">
                                                {{c.MobileElig}}
                                            </td>
                                            <td ng-show="$parent.Hierarchy.HasMobileTravel">
                                                {{c.TravelElig}}
                                            </td>
                                            <td ng-show="$parent.Hierarchy.HasVarible">
                                                {{c.VariableLiner}}
                                            </td>
                                            <td ng-show="$parent.Hierarchy.HasVarible">
                                                {{c.VariableWriteoff}}
                                            </td>
                                            <td ng-show="$parent.Hierarchy.HasFixedSummary">
                                                {{c.FixpayBasic}}
                                            </td>
                                            <td ng-show="$parent.Hierarchy.HasFixedIndividual">
                                                {{c.FixpayBasic}}
                                            </td>
                                            <td ng-show="$parent.Hierarchy.HasFixedIndividual">
                                                {{c.FixpayHra}}
                                            </td>
                                            <td ng-show="$parent.Hierarchy.HasFixedIndividual">
                                                {{c.FixpayOther}}
                                            </td>
                                            <td ng-show="$parent.Hierarchy.HasServiceCharge">
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </div>
                </div>
                
                 <!--Stakeholder Address details single-->
                <div class="panel" ng-show="$parent.Hierarchy.HasAddress">
                    <div class="panel-heading">
                        Address Details
                    </div>
                    <div class="row-fluid" ng-form="formAddress" id="stake-address-single" >
                       
                        <!--row for address type and is official-->
                        <div class="row-fluid">
                            <div class="control-group span6">
                                <label class="control-label">Address Type:</label>
                                   
                                <div class="controls">
                                    <select id="addressType" ng-required="$parent.Hierarchy.HasAddress" ng-model="Stakeholder.Address.AddressType" style="min-width: 200px;">
                                        <option value="Head Office">Head Office</option>
                                        <option value="Local Office">Local Office</option>
                                        <option value="Registered Office">Registered Office</option>
                                    </select>
                                </div>
                            </div>
                            <div class="control-group span6">
                                <label class="control-label">Is Communication Address:</label>
                                   
                                <div class="controls">
                                    <input type="checkbox" ng-model="Stakeholder.Address.IsOfficial"/>
                                </div>
                            </div>
                        </div>
                          
                        <!--row for line1 and line2-->
                        <div class="row-fluid">
                            <div class="control-group span6">
                                <label class="control-label">Line 1:</label>
                                   
                                <div class="controls">
                                    <input type="text" ng-required="$parent.Hierarchy.HasAddress" ng-model="Stakeholder.Address.Line1"/>
                                </div>
                            </div>
                               
                        </div>
                            
                        <div class="row-fluid">
                               
                            <div class="control-group span6">
                                <label class="control-label">Line 2:</label>
                                    
                                <div class="controls">
                                    <input type="text" ng-required="$parent.Hierarchy.HasAddress" ng-model="Stakeholder.Address.Line2"/>
                                </div>
                            </div>
                        </div>
                          
                        <!--row for line3-->
                        <div class="row-fluid">
                            <div class="control-group span6">
                                <label class="control-label">Line 3:</label>
                                    
                                <div class="controls">
                                    <input type="text" ng-model="Stakeholder.Address.Line3"/>
                                </div>
                            </div>
                        </div>
                          
                        <!--row for landline no and pincode-->
                        <div class="row-fluid">
                            <div class="control-group span6">
                                <label class="control-label">Pincode:</label>
                                    
                                <div class="controls">
                                    <input type="text" ng-required="$parent.Hierarchy.HasAddress" ng-pattern="/^[0-9][0-9][0-9][0-9][0-9][0-9]$/" ng-model="Stakeholder.Address.Pincode"/>
                                </div>
                            </div>

                            <div class="control-group span6">
                                <label class="control-label">Landline no:</label>
                                   
                                <div class="controls">
                                    <input type="text" ng-model="Stakeholder.Address.LandlineNo"/>
                                </div>
                            </div>
                               
                        </div>

                        <div class="row-fluid control-group span6" ng-show="$parent.Hierarchy.HasMultipleAddress">
                            <div class="controls">
                                <input type="button" ng-disabled="!formAddress.$valid" value="Add Address" class="btn btn-success" ng-click="AddAddress(Stakeholder)"/>
                            </div>
                        </div>
                            
                        <!--table for displaying added addresses-->
                        <div class="row-fluid" ng-show="AddressList.length>0">
                            <table class="table table-bordered table-hover">
                                <thead>
                                    <tr class="success">
                                        <th>Delete</th>
                                        <th>Address Type</th>
                                        <th>Is Official</th>
                                        <th>Line 1</th>
                                        <th>Line 2</th>
                                        <th>Line 3</th>
                                        <th>Landline No</th>
                                        <th>Pincode</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr class="info" ng-repeat="c in AddressList">
                                        <td>
                                            <div class="btn-group">
                                                <i class="btn icon-trash" data-ng-click="DeleteAddress($index)"></i>
                                            </div>
                                        </td>
                                        <td>{{c.AddressType}}</td>
                                        <td>{{c.IsOfficial}}</td>
                                        <td>{{c.Line1}}</td>
                                        <td>{{c.Line2}}</td>
                                        <td>{{c.Line3}}</td>
                                        <td>{{c.LandlineNo}}</td>
                                        <td>{{c.Pincode}}</td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>

                    </div>
                </div>
            
                <!--Save button-->
                <div class="row-fluid">
                    <div class="text-center">
                        <input type="submit" value="Save" ng-disabled="!(formhierarchy.$valid && formDetails.$valid && formReg.$valid && validateList($parent.Hierarchy))" class="btn btn-success" ng-click="SaveData(Stakeholder,$parent.Hierarchy)"/>
                    </div>
                </div>
            </div>
        </form>
    </div>