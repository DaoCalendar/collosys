@{
    ViewBag.Title = "File Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container-fluid" style="height: 600px;">
    <div class=" row-fluid" data-ng-controller="fileDetailsController" data-ng-cloak="" style="overflow: scroll; height: 500px;">
        <div>
            <div class="row-fluid text-center" data-ng-hide="fileDetailsList.length > 0">
                <label class="info">No File Details</label>
            </div>
            <table class="table table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Operations</th>
                        <th>Alias Name</th>
                        <th>Alias Description</th>
                        <th>SCB Systems</th>
                        <th>Frequency</th>
                        <th>Depends On Alias</th>
                        <th>Category</th>
                        <th>File Type</th>
                        <th>File Name</th>
                        <th>File Count</th>
                        <th>Date Format</th>
                        <th>Sheet Name</th>
                        <th>Skip Line</th>
                        <th>Used For</th>
                        <th>File Directory</th>
                        <th>Email Id</th>
                        <th>Start Date</th>
                        <th>End Date</th>
                        <th>Description</th>
                    </tr>
                    <tr>
                        <th>
                            <i class="btn icon-plus" data-ng-click="openModel()"></i>
                        </th>

                        <th>
                            <input type="text" data-ng-model="search.AliasName" />
                        </th>

                        <th>
                            <input type="text" data-ng-model="search.AliasDescription" />
                        </th>

                        <th>
                            <input type="text" data-ng-model="search.ScbSystems" />

                        </th>

                        <th>
                            <input type="text" data-ng-model="search.Frequency" />

                        </th>

                        <th>
                            <input type="text" data-ng-model="search.DependsOnAlias" />

                        </th>

                        <th>
                            <input type="text" data-ng-model="search.Category" />

                        </th>

                        <th>
                            <input type="text" data-ng-model="search.FileType" />

                        </th>

                        <th>
                            <input type="text" data-ng-model="search.FileName" />

                        </th>

                        <th>
                            <input type="text" data-ng-model="search.FileCount" />

                        </th>

                        <th>
                            <input type="text" data-ng-model="search.DateFormat" />

                        </th>

                        <th>
                            <input type="text" data-ng-model="search.SheetName" />

                        </th>

                        <th>
                            <input type="text" data-ng-model="search.SkipLine" />

                        </th>

                        <th>
                            <input type="text" data-ng-model="search.UsedFor" />

                        </th>

                        <th>
                            <input type="text" data-ng-model="search.FileDirectory" />

                        </th>

                        <th>
                            <input type="text" data-ng-model="search.EmailId" />

                        </th>

                        <th>
                            <input type="text" data-ng-model="search.StartDate" />

                        </th>

                        <th>
                            <input type="text" data-ng-model="search.EndDate" />

                        </th>


                        <th></th>

                    </tr>

                </thead>

                <tbody>
                    <tr data-ng-repeat="fileDetail in fileDetailsList | filter:search">
                        <td>
                            <div>
                                <div class="btn-group">
                                    <i class="btn icon-file-alt" data-ng-click="openModelData(fileDetail,true)"></i>
                                    <i class="btn icon-edit" data-ng-click="openModelData(fileDetail,false,$index)"></i>
                                    <i class="btn icon-trash" data-ng-show="true" data-ng-click="showDeleteModelPopup(fileDetail,$index)"></i>
                                </div>
                            </div>
                        </td>
                        <td>{{fileDetail.AliasName}}
                        </td>

                        <td>{{fileDetail.AliasDescription}}
                        </td>

                        <td>{{fileDetail.ScbSystems}}
                        </td>

                        <td>{{fileDetail.Frequency}}
                        </td>

                        <td>{{fileDetail.DependsOnAlias }}
                        </td>

                        <td>{{fileDetail.Category}}
                        </td>

                        <td>{{fileDetail.FileType}}
                        </td>

                        <td>{{fileDetail.FileName}}
                        </td>

                        <td>{{fileDetail.FileCount}}
                        </td>

                        <td>{{fileDetail.DateFormat}}
                        </td>

                        <td>{{fileDetail.SheetName}} 
                        </td>

                        <td>{{fileDetail.SkipLine}}
                        </td>

                        <td>{{fileDetail.UsedFor}}
                        </td>

                        <td>{{fileDetail.FileDirectory}}
                        </td>

                        <td>{{fileDetail.EmailId}}
                        </td>

                        <td>
                            <!--Start Date -->
                            <div class="input-append">
                                <input type="text" name="StartDate" class="input-medium" data-ng-disabled="true" data-ng-model="fileDetail.StartDate" bs-datepicker />
                                <button type="button" class="btn" data-toggle="datepicker"><i class="icon-calendar"></i></button>
                            </div>
                        </td>
                        <td>
                            <!--End Date -->
                            <div class="input-append">
                                <input type="text" name="EndtDate" class="input-medium" ui-validate=" '$value > fileDetail.StartDate'" data-ng-disabled="true" data-ng-model="fileDetail.EndDate" bs-datepicker />
                                <button type="button" class="btn" data-toggle="datepicker"><i class="icon-calendar"></i></button>
                            </div>
                        </td>

                        <td>{{fileDetail.Description}}
                        </td>


                    </tr>
                </tbody>
            </table>
        </div>

        <div modal="showDeleteModel" close="noToDelete()" options="modelOption">
            <div class="modal-header well-small2">
                Confirm to Delete
            </div>
            <div class="modal-body">
                Are you sure to Delete?
            </div>
            <div class="modal-footer well">
                <div class="row-fluid">
                    <span class="left">
                        <input type="button" value="Yes" class="btn btn-success" data-ng-click="yesToDelete()" />
                        <input type="button" value="No" class="btn btn-info" data-ng-click="noToDelete()" />
                    </span>
                </div>
            </div>
        </div>

        <div style="width: 300px">
            <form name="form" novalidate>
                <div>
                    <div modal="shouldBeOpen" close="closeModel()" options="modelOption">
                        <div class="modal-header well">
                            {{modelTitle}}
                        </div>
                        <div data-ng-form="formAddfileDetail" class="form-horizontal">

                            <div class="modal-body">

                                <!--Alias Name -->
                                <cs-enum-field options="FileDetailsModel.AliasName" ng-model="fileDetail.AliasName" ng-show="!isReadOnly"></cs-enum-field>

                                <!--AliasDescription -->
                                <cs-textarea-field options="FileDetailsModel.AliasDescription" ng-model="fileDetail.AliasDescription" ng-show="!isReadOnly"></cs-textarea-field>

                                <!--SCB Systems -->
                                <cs-enum-field options="FileDetailsModel.ScbSystems" ng-model="fileDetail.ScbSystems" ng-show="!isReadOnly"></cs-enum-field>

                                <!--File Frequency -->
                                <cs-enum-field options="FileDetailsModel.Frequency" ng-model="fileDetail.Frequency" ng-show="!isReadOnly"></cs-enum-field>

                                <!--DependsOnAlias -->
                                <cs-enum-field options="FileDetailsModel.DependsOnAlias" ng-model="fileDetail.DependsOnAlias" ng-show="!isReadOnly"></cs-enum-field>

                                <!--Category -->
                                <cs-enum-field options="FileDetailsModel.Category" ng-model="fileDetail.Category" ng-show="!isReadOnly"></cs-enum-field>

                                <!--File Type -->
                                <cs-enum-field options="FileDetailsModel.FileType" ng-model="fileDetail.FileType" ng-show="!isReadOnly"></cs-enum-field>

                                <!-- File Name -->
                                <cs-text-field options="FileDetailsModel.FileName" ng-model="fileDetail.FileName" ng-show="!isReadOnly"></cs-text-field>

                                <!--File Count -->

                                <cs-number-field options="FileDetailsModel.FileCount" ng-model="fileDetail.FileCount" ng-show="!isReadOnly"></cs-number-field>

                                <!--Date Format -->
                                <cs-select-field options="FileDetailsModel.DateFormat" ng-model="fileDetail.DateFormat" cs-repeat="'dateFormats'"
                                    text-field="format" value-field="format" ng-show="!isReadOnly"></cs-select-field>

                                <!--Sheet Name -->
                                <cs-text-field options="FileDetailsModel.SheetName" ng-model="fileDetail.SheetName" ng-show="!isReadOnly"></cs-text-field>

                                <!--Skip Line -->
                                <cs-number-field options="FileDetailsModel.SkipLine" ng-model="fileDetail.SkipLine" ng-show="!isReadOnly"></cs-number-field>

                                <!--Used for -->
                                <cs-enum-field options="FileDetailsModel.UsedFor" ng-model="fileDetail.UsedFor" ng-show="!isReadOnly"></cs-enum-field>

                                <!--File Directory -->
                                <cs-text-field options="FileDetailsModel.FileDirectory" ng-model="fileDetail.FileDirectory" ng-show="!isReadOnly"></cs-text-field>

                                <!--Email ID -->
                                <cs-email-field options="FileDetailsModel.EmailId" ng-model="fileDetail.EmailId" ng-show="!isReadOnly"></cs-email-field>

                                <!--Start Date -->
                                <cs-date-field options="FileDetailsModel.StartDate" ng-model="fileDetail.StartDate" ng-show="!isReadOnly"></cs-date-field>

                                <!--End Date -->
                                <cs-date-field options="FileDetailsModel.EndDate" ng-model="fileDetail.EndDate" ng-show="!isReadOnly"></cs-date-field>

                                <!--Description -->
                                <cs-text-field options="FileDetailsModel.Description" ng-model="fileDetail.Description" ng-show="!isReadOnly"></cs-text-field>

                                <div class="modal-footer control-group">
                                    <div class="controls">
                                        <cs-button type="save" ng-click="saveFileDetails(fileDetail)" ng-disabled="!formAddfileDetail.$valid"></cs-button>
                                        <cs-button type="cancel" ng-click="closeModel()" ng-show="!isReadOnly"></cs-button>
                                        <cs-button type="close" ng-click="closeModel()" ng-show="isReadOnly"></cs-button>

                                    </div>
                                </div>

                            </div>

                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>
</div>



@section scripts
{
    <script src="~/Areas/FileUploader/jsControllers/fileDetails.js"></script>
    <script src="~/app/fileupload/models.js"></script>
}

